@using Microsoft.AspNet.Identity

<ul class="navbar-nav">
    @if (Request.IsAuthenticated)
    {
        <li nav-item>@Html.ActionLink("Sign out", "SignOut", "Account", new { }, new { @class = "nav-link" })</li>
    }
    else
    {
        <li nav-item>@Html.ActionLink("Register", "Register", "Account", new { }, new { @class = "nav-link" })</li>
        <li nav-item> @Html.ActionLink("Log in", "Login", "Account", new { }, new { @class = "nav-link" })</li>
    }
</ul>